#!/bin/bash

TEST=""

if [[ "$*" == *"gen_more"* ]]
  then
    for i in {6..1000..1}; do
      echo -n $TEST > ./tests/sendme_"$i"
    done
  echo "Tanti test creati"
elif [[ "$*" == *"gen"* ]]
  then
    for i in {6..100..1}; do
      echo -n $TEST > ./tests/sendme_"$i"
    done
  echo "Test creati"
fi

if [[ "$*" ==  *"clean"* ]]
  then
    rm -f ./tests/*_out
    echo "Test out eliminati"
fi

if [[ "$*" == *"del"* ]]
  then
    rm -f ./tests/sendme_{6..2000}
    echo "Test > 6 eliminati"
fi